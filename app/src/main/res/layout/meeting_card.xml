<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:id="@+id/oneMeetingCard"
    android:layout_height="match_parent"
    android:orientation="vertical"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">


    <androidx.cardview.widget.CardView
        android:layout_width="373dp"
        android:layout_height="69dp"
        android:layout_gravity="center"
        android:backgroundTint="#6300BCD4"
        app:cardBackgroundColor="#00BCD4">

        <Button
            android:id="@+id/buttonUsernameMeeting"
            style="@style/Widget.Material3.Button.TextButton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:contentDescription="username"
            android:text="[Name]"
            android:textColor="#ECE4E4"
            app:iconTint="#2196F3" />

        <ImageButton
            android:id="@+id/imageDeleteMeeting"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical|end"
            android:layout_marginRight="10dp"
            android:backgroundTint="#00FFFFFF"
            android:contentDescription="delete"
            app:srcCompat="@android:drawable/ic_menu_delete" />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="68dp"
            android:layout_height="match_parent"
            app:srcCompat="@drawable/_60_f_346839683_6napzbhpskipb8pmawufkc7c5ed7wyws" />

    </androidx.cardview.widget.CardView>

</LinearLayout>